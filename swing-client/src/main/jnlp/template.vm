<?xml version="1.0" encoding="utf-8"?>
<jnlp
        spec="1.0+"
        codebase="http://piraten.raffael.ch/pivote-java"
        href="launch.jnlp">
    <information>
        <title>ch.piratenpartei.pivote:pivote-java</title>
        <vendor>Raffael Herzog</vendor>
        <description>A Prototype for a Pi-Vote client written in Java</description>
        <offline-allowed/>
    </information>
    <security>
        <all-permissions/>
    </security>
    <resources>
        <j2se version="1.6+"/>
        <property name="ch.piratenpartei.pivote.development" value="true"/>

        $dependencies
    </resources>
    <application-desc main-class="$mainClass"/>
</jnlp>
